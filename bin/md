#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"

  block runmany 4 'ln -vnfs $1/content "${BOARD_PATH}/content/${1##*/}"'

  rm -rf .public
  block runmany 4 'if test -f $1/config.json; then block compile docs $1; fi || true'
  block compile docs
}

source sub "$BASH_SOURCE" "$@"
